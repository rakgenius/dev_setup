<plugins>
  <plugin>
    <groupId>com.google.cloud.tools</groupId>
    <artifactId>jib-maven-plugin</artifactId>
    <version>1.8.0</version>
    <configuration>
	<container>
		<useCurrentTimestamp>true</useCurrentTimestamp>
	</container>
        <to>
         	<image>springbootjib</image>
        </to>
   </configuration>
  </plugin>
</plugins>



mvn compile jib:dockerBuild
